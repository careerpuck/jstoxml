  "description": "Converts JavaScript/JSON to XML (for RSS, Podcasts, AMP, etc.)",
  "homepage": "http://github.com/davidcalhoun/jstoxml",
  "keywords": [
    "amp",
    "google amp",
    "json",
    "podcast",
    "rss",
    "sitemap",
    "site map",
    "xml"
  ],
  "main": "dist/jstoxml.js",
